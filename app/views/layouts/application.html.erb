<!DOCTYPE html>
<html>
  <head>
    <title>PersonalAssessment</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if @current_user %>
      <%= link_to 'Log-out', logout_path %>
      <% else %>
      <p><%= link_to 'Signup', new_user_path %>, <%= link_to 'login', login_path %>, or login with your <%= link_to "Google Account", "/auth/google_oauth2" %> <p>
    <% end%>




    <%= yield %>
  </body>
</html>
